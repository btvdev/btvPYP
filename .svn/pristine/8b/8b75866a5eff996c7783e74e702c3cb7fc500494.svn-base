//		sampleMatchList = merge(sampleMatchList);
		if(null != sampleMatchList && sampleMatchList.size()>0){
//			sampleMatchList.get(0).setNeedRed(1);
			for(int i=0;i<sampleMatchList.size();i++){
				//相邻两条流水单记录如果前后时间没有完全咬合的显示处理逻辑
				if(i!=0){
